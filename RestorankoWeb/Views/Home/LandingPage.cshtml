@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model List<API.Models.User>

<h1>User List</h1>

<table class="table">
    <thead>
        <tr>
            <th>User ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Iduser</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
              
            </tr>
        }
    </tbody>
</table>

